{
  "name": "m1naret/lumen-graphql",
  "description": "Lumen wrapper for PHP GraphQL",
  "type": "library",
  "keywords": [
    "framework",
    "lumen",
    "graphql"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "laravel/lumen-framework": "~5.5",
    "illuminate/support": "~5.5",
    "webonyx/graphql-php": "^0.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5|~6.0"
  },
  "autoload": {
    "psr-0": {
      "M1naret\\GraphQL\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
